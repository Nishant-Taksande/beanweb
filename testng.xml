<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="beanlogin" parallel="tests" thread-count="3"
	data-provider-thread-count="1">

	<test name="Login Functionality Test">
		<classes>
			<class name="regression.LoginFunctionalityTest" />
		</classes>
	</test>

	<test name="Forgot password Functionality Test">
		<classes>
			<class name="regression.ForgotPasswordFunctinalityTest" />
		</classes>
	</test>
	<test name="Forgot password Ui Test">
		<classes>
			<class name="regression.ForgotPasswordUiTest" />
		</classes>
	</test>
	<test name="Login Ui Test">
		<classes>
			<class name="regression.LoginPageUiTest" />
		</classes>
	</test>
	<test name="Registration Functionality Active User Test">
		<classes>
			<class name="regression.RegistrationFuctionalityActiveUserTest" />
		</classes>
	</test>
	<test name="Registration Functionality Inactive User Test">
		<classes>
			<class name="regression.RegistrationFunctionalityInactiveUserTest" />
		</classes>
	</test>
	<test name="Registration Ui Test">
		<classes>
			<class name="regression.RegistrationPageUiTest" />
		</classes>
	</test>
</suite>
